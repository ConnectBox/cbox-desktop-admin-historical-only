{
  "name": "cbox-desktop-admin-app",
  "productName": "ConnectBox Desktop Admin",
  "description": "Admin Application for configuring a ConnectBox",
  "version": "0.2.12",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "create-react-class": "^15.6.3",
    "decompress-zip": "^0.3.1",
    "drivelist": "^6.4.3",
    "electron": "1.7.9",
    "electron-dl": "^1.12.0",
    "electron-download-manager": "^2.2.0",
    "electron-platform": "^1.2.0",
    "emotion": "^10.0.1",
    "fs-extra": "^7.0.1",
    "i18next": "^12.1.0",
    "i18next-electron-language-detector": "^0.0.10",
    "i18next-xhr-backend": "^1.5.1",
    "immutability-helper": "^2.7.1",
    "immutable": "^4.0.0-rc.12",
    "localforage": "^1.7.2",
    "lodash": "^4.17.10",
    "mdi-material-ui": "^5.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-data-grid": "^6.0.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^10.0.0",
    "react-i18next": "^8.3.8",
    "react-live": "^1.11.0",
    "react-photo-gallery": "^6.0.28",
    "react-player": "^1.6.4",
    "react-reader": "^0.17.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.2",
    "react-tappable": "^1.0.4",
    "shorthash": "^0.0.2",
    "soundmanager2": "^2.97.20170602",
    "typeface-roboto": "^0.0.54",
    "url": "^0.11.0",
    "verge": "^1.10.2"
  },
  "devDependencies": {
    "electron-rebuild": "^1.8.1",
    "electron-winstaller": "^2.6.4",
    "foreman": "^3.0.1",
    "react-scripts": "^2.1.1"
  },
  "homepage": "./",
  "main": "electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "package-mac": "electron-packager . --overwrite --platform=darwin --ignore=cred.js --arch=x64 --icon=./icons/mac/CBox.ico --prune=true --out=release-builds",
    "package-win32": "electron-packager . cbox-desktop-admin-app --overwrite --platform=win32 --ignore=cred.js --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=Qombi --version-string.FileDescription=V2.12 --version-string.ProductName=\"CBox Desktop Admin App\"",
    "package-win": "electron-packager . cbox-desktop-admin-app --overwrite --platform=win32 --ignore=cred.js --arch=x64 --icon=./icons/win/CBox.ico --prune=true --out=release-builds --version-string.CompanyName=Qombi --version-string.FileDescription=V2.12 --version-string.ProductName=\"CBox Desktop Admin App\"",
    "package-linux": "electron-packager . cbox-desktop-admin-app --overwrite --asar --platform=linux --ignore=cred.js --arch=x64 --icon=icons/linux/CBox.png --prune=true --out=release-builds",
    "package-linux-test": "electron-packager . cbox-desktop-admin-app --overwrite --platform=linux --ignore=cred.js --arch=x64 --icon=icons/linux/CBox.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-debian-installer": "electron-installer-debian --src release-builds/cbox-desktop-admin-app-linux-x64/ --arch amd64 --config debian.json",
    "create-debian-installer-x64": "electron-installer-debian --src release-builds/cbox-desktop-admin-app-linux-x64/ --arch x64 --config debian.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
