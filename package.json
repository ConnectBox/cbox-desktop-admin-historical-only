{
  "name": "cbox-desktop-admin-app",
  "productName": "ConnectBox Desktop Admin",
  "description": "Admin Application for configuring a ConnectBox",
  "version": "0.2.17",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "adm-zip": "^0.4.13",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "create-react-class": "^15.6.3",
    "decompress-zip": "^0.3.1",
    "drivelist": "^6.4.3",
    "electron": "7.2.4",
    "electron-dl": "^1.14.0",
    "electron-download-manager": "^2.2.0",
    "electron-platform": "^1.2.0",
    "electron-push-receiver": "^2.1.2",
    "emotion": "^10.0.14",
    "fs-extra": "^8.1.0",
    "i18next": "^17.0.6",
    "i18next-electron-language-detector": "^0.0.10",
    "i18next-xhr-backend": "^3.0.0",
    "immutability-helper": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "localforage": "^1.7.2",
    "lodash": "^4.17.15",
    "mdi-material-ui": "^6.2.0",
    "node-machine-id": "^1.1.12",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^10.0.0",
    "react-i18next": "^10.11.4",
    "react-live": "^2.1.2",
    "react-player": "^1.11.1",
    "react-reader": "^0.17.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "react-slick": "^0.24.0",
    "react-tappable": "^1.0.4",
    "shorthash": "^0.0.2",
    "soundmanager2": "^2.97.20170602",
    "typeface-roboto": "^0.0.75",
    "url": "^0.11.0",
    "verge": "^1.10.2"
  },
  "devDependencies": {
    "electron-rebuild": "^1.8.5",
    "electron-winstaller": "^3.0.4",
    "foreman": "^3.0.1",
    "react-scripts": "^3.0.1"
  },
  "homepage": "./",
  "main": "electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "package-mac": "electron-packager . --overwrite --platform=darwin --ignore=cred.js --ignore=^/src --ignore=^/public --arch=x64 --icon=./icons/mac/CBox.ico --prune=true --out=release-builds",
    "package-win32": "electron-packager . cbox-desktop-admin-app --overwrite --platform=win32 --ignore=cred.js --ignore=^/src --ignore=^/public --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=Qombi --version-string.FileDescription=V2.17 --version-string.ProductName=\"CBox Desktop Admin App\"",
    "package-win": "electron-packager . cbox-desktop-admin-app --overwrite --platform=win32 --ignore=cred.js --ignore=^/src --ignore=^/public --arch=x64 --icon=./icons/win/CBox.ico --prune=true --out=release-builds --version-string.CompanyName=Qombi --version-string.FileDescription=V2.17 --version-string.ProductName=\"CBox Desktop Admin App\"",
    "package-linux": "electron-packager . cbox-desktop-admin-app --overwrite --asar --platform=linux --ignore=cred.js --ignore=^/src --ignore=^/public --arch=x64 --icon=icons/linux/CBox.png --prune=true --out=release-builds",
    "package-linux-test": "electron-packager . cbox-desktop-admin-app --overwrite --platform=linux --ignore=cred.js --ignore=^/src --ignore=^/public --arch=x64 --icon=icons/linux/CBox.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-debian-installer": "electron-installer-debian --src release-builds/cbox-desktop-admin-app-linux-x64/ --arch amd64 --config debian.json",
    "create-debian-installer-x64": "electron-installer-debian --src release-builds/cbox-desktop-admin-app-linux-x64/ --arch x64 --config debian.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
